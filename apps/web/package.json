{
  "name": "@dish/web",
  "version": "0.0.0",
  "private": true,
  "main": "app.json",
  "workspaces": {
    "nohoist": [
      "**"
    ]
  },
  "scripts": {
    "generate": "gqless generate",
    "start:android": "react-native run-android",
    "start:ios": "react-native run-ios",
    "start:web": "webpack-dev-server --port 19006",
    "start:web:worker": "cd ../web-serve && yarn start",
    "start:web:prod": "node --version && node _/server.js",
    "start": "yarn start:web",
    "start:react-native": "react-native start",
    "test": "true",
    "postinstall": "jetify && rm -r node_modules/@apollo/client/node_modules/react || true && patch-package",
    "build": "tsc -b tsconfig.build.json",
    "build:web": "yarn build && (NODE_ENV=production yarn build:web:client) & (NODE_ENV=production yarn build:web:server) & wait",
    "build:web:server": "TARGET=ssr webpack",
    "build:web:client": "TARGET=client webpack",
    "build:web:worker": "TARGET=worker webpack",
    "build:web:watch": "(TARGET=ssr webpack --watch) & (TARGET=client webpack --watch) & wait",
    "ios": "react-native run-ios",
    "android": "react-native run-android"
  },
  "dependencies": {
    "@apollo/client": "3.0.0-beta.43",
    "@dish/common-web": "*",
    "@dish/models": "*",
    "@expo/vector-icons ": "10.0.6",
    "@o/fast-compare": "2.7.1",
    "expo": "^36.0.2",
    "expo-linear-gradient": "^8.1.0",
    "fuse.js": "5.0.10-beta",
    "gqless": "^0.0.1-alpha.28",
    "mapkit-typescript": "^5.18.1",
    "overmind": "^23.0.1",
    "overmind-graphql": "^3.0.1",
    "overmind-react": "^24.0.1",
    "page": "1.11.5",
    "query-string": "6.11.1",
    "react": "0.0.0-experimental-aae83a4b9",
    "react-dom": "0.0.0-experimental-aae83a4b9",
    "react-helmet": "^5.2.1",
    "react-native": "~0.61.4",
    "react-native-animatable": "^1.3.3",
    "react-native-dotenv": "^0.2.0",
    "react-native-easy-toast": "^1.2.0",
    "react-native-gesture-handler": "1.5.0",
    "react-native-popover-view": "^2.0.7",
    "react-native-reanimated": "1.4.0",
    "react-native-side-menu": "^1.1.3",
    "react-native-unimodules": "~0.7.0",
    "react-native-web": "0.11.7",
    "react-storage-hooks": "^4.0.0",
    "react-tiny-popover": "^4.0.0"
  },
  "devDependencies": {
    "@types/requestidlecallback": "0.3.1",
    "@expo/webpack-config": "^0.11.8",
    "@gqless/cli": "^0.0.1-alpha.30",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.2.0",
    "@types/jsdom": "^16.2.0",
    "@types/lodash": "4.14.149",
    "@types/page": "1.8.0",
    "@types/react": "16.8.7",
    "@types/react-native": "0.62.2",
    "babel-jest": "~24.9.0",
    "babel-preset-expo": "^8.0.0",
    "body-parser": "^1.19.0",
    "canvas": "^2.6.1",
    "expo-cli": "^3.16.1",
    "express": "^4.17.1",
    "isomorphic-fetch": "2.2.1",
    "jest": "~24.9.0",
    "jest-expo": "~36.0.1",
    "jsdom": "^16.2.2",
    "metro-react-native-babel-preset": "^0.59.0",
    "moment": "^2.24.0",
    "overmind-devtools": "^24.0.1",
    "patch-package": "^6.2.1",
    "postinstall-postinstall": "^2.0.0",
    "preact": "^10.3.4",
    "preact-responder-event-plugin": "^1.0.16",
    "prettyjson": "^1.2.1",
    "react-refresh": "^0.8.1",
    "react-test-renderer": "~16.9.0",
    "testcafe": "^1.8.4",
    "typescript": "3.8.3",
    "use-expo": "1.0.1",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "import-sort-style-module": "^6.0.0",
    "prettier-plugin-import-sort": "^0.0.4",
    "webpack-hot-middleware": "^2.25.0"
  },
  "resolutions": {
    "react-native": "0.61.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
