{
  "name": "@dish/web",
  "version": "0.0.0",
  "private": true,
  "main": "app.json",
  "workspaces": {
    "nohoist": [
      "**"
    ]
  },
  "scripts": {
    "generate": "node _/src/generate.js",
    "start:android": "react-native run-android",
    "start:ios": "react-native run-ios",
    "start:web": "VERBOSE=1 webpack-dev-server --port 19006 --progress",
    "start:devtools": "npx overmind-devtools@24.0.0",
    "start:web:no-extract": "NO_EXTRACT=1 yarn start:web",
    "start:web:webpack5": "VERBOSE=1 webpack serve --port 19006 --progress",
    "start:web:prod": "LEGACY=1 NODE_ENV=production VERBOSE=1 NO_MINIFY=1 webpack-dev-server --port 19006 --progress",
    "start:web:prod:webpack5": "VERBOSE=1 NO_MINIFY=1 NODE_ENV=production webpack serve --port 19006 --progress",
    "start:web:worker": "cd ../web-serve && yarn start",
    "start:web:ssr": "node _/web/server.js",
    "start": "yarn start:web",
    "start:prod": "yarn start:web:prod",
    "start:prod:no-min": "NO_MINIFY=1 yarn start:web:prod",
    "start:ssr": "yarn start:web:ssr",
    "start:react-native": "react-native start",
    "test": "true",
    "postinstall": "./etc/postinstall.sh",
    "build": "npm-run-all --parallel build:types build:output",
    "build:types": "tsc",
    "build:output": "tsc -p tsconfig.build.json",
    "watch": "npm-run-all --parallel watch:types watch:output",
    "watch:output": "tsc -p tsconfig.build.json --watch --preserveWatchOutput",
    "watch:types": "tsc --watch --preserveWatchOutput",
    "watch:web:ssr": "yarn build:web:ssr --watch",
    "build:web": "./etc/build_web.sh",
    "build:web:ssr": "VERBOSE=1 NODE_ENV=production TARGET=ssr webpack",
    "build:web:client": "VERBOSE=1 NODE_ENV=production webpack",
    "build:web:client:analyze": "ONLY_MODERN=1 ANALYZE_BUNDLE=1 yarn build:web:client",
    "build:web:worker": "TARGET=worker webpack",
    "build:web:watch": "(TARGET=ssr webpack --watch) & (TARGET=client webpack --watch) & wait",
    "build:web:webpack-tsickle": "node --max-old-space-size=12000 tscc.js"
  },
  "dependencies": {
    "@dish/async": "*",
    "@dish/graph": "*",
    "@dish/helpers": "*",
    "@dish/ui": "*",
    "@expo/html-elements": "0.0.0",
    "@o/fast-compare": "2.7.6",
    "expo-linear-gradient": "^8.1.0",
    "flexsearch": "^0.6.32",
    "immer": "^7.0.5",
    "mapbox-gl": "1.11.1",
    "overmind": "23.1.0-1589389364682",
    "overmind-react": "24.1.0-1589389364682",
    "page": "1.11.5",
    "query-string": "6.11.1",
    "react": "*",
    "react-dom": "*",
    "react-error-boundary": "^2.2.1",
    "react-feather": "2.0.4",
    "react-helmet": "^6.1.0",
    "react-laag": "^1.7.3",
    "react-native": "^0.63.2",
    "react-native-dotenv": "^0.2.0",
    "react-native-unimodules": "~0.7.0",
    "react-native-gesture-handler": "1.5.0",
    "react-reparenting": "^0.4.0",
    "react-native-web": "^0.13.8",
    "react-storage-hooks": "^4.0.1",
    "resize-observer-polyfill": "1.5.1"
  },
  "devDependencies": {
    "circular-dependency-plugin": "^5.2.0",
    "@dish/babel-preset": "*",
    "@dish/use-store": "*",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.1",
    "@types/jsdom": "^16.2.0",
    "@types/lodash": "^4.14.155",
    "@types/page": "1.8.0",
    "@types/mapbox-gl": "^1.11.1",
    "@types/react": "*",
    "@types/react-native": "*",
    "@types/requestidlecallback": "0.3.1",
    "@webpack-cli/serve": "1.0.1-alpha.5",
    "array-flat-polyfill": "^1.0.1",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "browserslist-useragent": "3.0.3",
    "canvas": "2.6.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "dedupe-parent-css-from-chunks-webpack-plugin": "^3.0.0",
    "express": "^4.17.1",
    "extract-css-chunks-webpack-plugin": "^4.7.5",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "inspectpack": "4.4.0",
    "isomorphic-unfetch": "^3.0.0",
    "jsdom": "^16.2.2",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.0.0",
    "preact": "^10.3.4",
    "preact-responder-event-plugin": "^1.0.16",
    "style-loader": "1.2.1",
    "terser-webpack-plugin": "^3.0.2",
    "testcafe": "^1.8.4",
    "type-fest": "0.13.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-common-shake": "^2.1.0",
    "webpack-dev-server": "^3.11.0"
  }
}
