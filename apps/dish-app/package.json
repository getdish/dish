  "scripts": {
    "start": "TARGET=native react-native start",
    "android": "TARGET=native DISH_ENV=development react-native run-android --no-packager",
    "ios:run": "TARGET=native react-native run-ios --no-packager",
    "ios": "DISH_ENV=development yarn ios:run --simulator=\"iPhone 11 Pro\"",
    "ios:prod": "yarn ios:run --configuration Release",
    "ios:device": "yarn ios:run --device",
    "ios:device:prod": "yarn ios:run --device --configuration Release",
    "web": "DISH_ENV=development TARGET=web VERBOSE=1 webpack-dev-server --port 19006 --progress",
    "web:no-extract": "NO_EXTRACT=1 yarn web",
    "web:webpack5": "VERBOSE=1 webpack serve --port 19006 --progress",
    "web:prod": "TARGET=web LEGACY=1 NODE_ENV=production VERBOSE=1 webpack-dev-server --port 19006 --progress",
    "web:prod:webpack5": "TARGET=web VERBOSE=1 NO_MINIFY=1 NODE_ENV=production webpack serve --port 19006 --progress",
    "web:worker": "cd ../web-serve && yarn start",
    "web:ssr": "node _/web/server.js",
    "preinstall": "../../bin/preinstall.sh; node ./etc/update_package_json_workspace_hoist.js",
    "postinstall": "../../bin/postinstall.sh; yarn patch-package; expo-yarn-workspaces postinstall",
    "devtools": "npx overmind-devtools@26.0.1",
    "start:prod": "NO_MINIFY=1 yarn web:prod",
    "start:prod:min": "yarn web:prod",
    "start:ssr": "yarn web:ssr",
    "start:ssr:development": "NODE_ENV=development yarn web:ssr",
    "start:react-native": "react-native start",
    "test": "true",
    "build": "npm-run-all --parallel build:types build:output",
    "build:types": "tsc",
    "build:output": "tsc -p tsconfig.build.json",
    "build:splash": "expo-splash-screen -r contain '#25111A' ./assets/splash.png",
    "build:icon": "react-native-svg-app-icon",
    "watch": "npm-run-all --parallel watch:types watch:output",
    "watch:output": "tsc -p tsconfig.build.json --watch --preserveWatchOutput",
    "watch:types": "tsc --watch --preserveWatchOutput",
    "watch:web:ssr": "yarn build:web:ssr --watch",
    "build:web": "./etc/build_web.sh",
    "build:web:ssr": "TARGET=ssr VERBOSE=1 NODE_ENV=production webpack",
    "build:web:ssr:development": "SSR_CLIENT=1 TARGET=ssr VERBOSE=1 NODE_ENV=development webpack",
    "build:web:client": "SSR_CLIENT=1 TARGET=web VERBOSE=1 NODE_ENV=production webpack",
    "build:web:client:analyze": "ONLY_MODERN=1 ANALYZE_BUNDLE=1 yarn build:web:client",
    "build:web:worker": "TARGET=worker webpack",
    "build:web:watch": "(TARGET=ssr webpack --watch) & (TARGET=client webpack --watch) & wait"
  },
