+export function getFilter(filter) {
+    if (!Array.isArray(filter) || filter.length === 0) {
+        return [];
+    }
+    return filter;
+}
diff --git a/b/javascript/utils/geoUtils.js b/b/javascript/utils/geoUtils.js
