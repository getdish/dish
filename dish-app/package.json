  "scripts": {
    "etc:colorize": "eval $(../dishctl.sh yaml_to_env) && DEBUG=1 HASURA_SECRET=$TF_VAR_HASURA_GRAPHQL_ADMIN_SECRET node -r esbuild-register ./etc/add_colors_to_tiles.ts",
    "etc:colorize:staging": "NODE_ENV=staging yarn etc:colorize",
    "start": "TARGET=native react-native start",
    "preinstall": "node ./etc/update_package_json_workspace_hoist.js",
    "postinstall": "patch-package; expo-yarn-workspaces postinstall",
    "web": "eval $(../dishctl.sh yaml_to_env) && LOG_LEVEL=1 DO_SPACES_ID=$TF_VAR_DO_SPACES_ID DO_SPACES_SECRET=$TF_VAR_DO_SPACES_SECRET SENDGRID_API_KEY=$TF_VAR_SENDGRID_API_KEY DO_SPACES_ID=$TF_VAR_DO_SPACES_ID DO_SPACES_SECRET=$TF_VAR_DO_SPACES_SECRET DISH_ENV=development LOG_FETCH=1 yarn dish start",
    "web:https": "sudo dish start --https --host d1sh.com --port 443",
    "web:debug": "DEBUG_WEBPACK=1 node --max_old_space_size=8192 --inspect ../node_modules/.bin/dish start --serial",
    "web:prod": "LOG_LEVEL=1 DEBUG_PATHS=1 VERBOSE=1 yarn web --prod",
    "web:prod:dev": "LOG_LEVEL=1 DEBUG_PATHS=1 VERBOSE=1 yarn web --prod --no-optimize",
    "web:serve": "dish serve",
    "web:build": "dish build",
    "web:build:analyze": "ANALYZE_BUNDLE=1 yarn web:build",
    "web:no-extract": "NO_EXTRACT=1 yarn web",
    "web:ssr": "PORT=80 node _/web/server.js",
    "web:ssr:development": "NODE_ENV=development yarn web:ssr",
    "ios:install": "cd ios && arch -x86_64 pod install",
    "ios:run": "TARGET=native react-native run-ios --no-packager",
    "ios": "DISH_ENV=development yarn ios:run --simulator=\"iPhone 11 Pro\"",
    "ios:prod": "yarn ios:run --configuration Release",
    "ios:device": "yarn ios:run --device",
    "ios:device:prod": "yarn ios:run --device --configuration Release",
    "android": "TARGET=native DISH_ENV=development react-native run-android --no-packager",
    "dev:react": "npx react-devtools",
    "test": "true",
    "build": "true",
    "build:docker": "cd .. && docker build -t dish/app -f dish-app/Dockerfile .",
    "build:types": "tsc",
    "build:output": "tsc -p tsconfig.build.json",
    "build:splash": "expo-splash-screen --image-resize-mode contain -b '#000000' -i src/assets/splash.png",
    "build:icon": "npx react-native-svg-app-icon",
    "watch": "tsc --watch --preserveWatchOutput",
    "generate:tags": "../dishctl.sh dish_app_generate_tags"
  },
