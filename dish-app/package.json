{
  "name": "@dish/app",
  "private": true,
  "version": "0.0.0",
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "expo-yarn-workspaces": {
    "symlinks": [
      "expo-constants"
    ]
  },
  "scripts": {
    "etc:colorize": "../dishctl.sh run 'DEBUG=1 node -r esbuild-register ./etc/add_colors_to_tiles.ts'",
    "postinstall": "./etc/postinstall.sh",
    "etc:colorize:staging": "NODE_ENV=staging yarn etc:colorize",
    "app": "LOG_LEVEL=1 TARGET=native NODE_ENV=development react-native start",
    "app:build": "npx eas build --platform ios --non-interactive",
    "app:reset": "watchman watch-del-all && yarn app --reset-cache",
    "app:native": "LOCAL_HOST=$(ipconfig getifaddr en0) yarn app",
    "app:native:prod": "NODE_ENV=production yarn app:native",
    "app:native:reset": "watchman watch-del-all && yarn app:native --reset-cache",
    "web": "DISH_ENV=development ../dishctl.sh run 'yarn dish start'",
    "web:reset": "NODE_ENV=development ../dishctl.sh run 'yarn dish start --reset-cache'",
    "web:native": "DISH_ENV=development ../dishctl.sh run 'yarn dish start --local-proxy'",
    "web:native:reset": "DISH_ENV=development ../dishctl.sh run 'yarn dish start --local-proxy --reset-cache'",
    "web:https": "sudo dish start --https --host d1sh.com --port 443",
    "web:debug": "DEBUG_WEBPACK=1 node --max_old_space_size=8192 --inspect ../node_modules/.bin/dish start --serial",
    "web:prod": "DISH_ENV=production ../dishctl.sh run 'yarn dish start --prod'",
    "web:prod:dev": "DISH_ENV=production ../dishctl.sh run 'yarn dish start --prod --no-optimize'",
    "web:serve": "dish serve",
    "web:build": "dish build",
    "web:build:analyze": "ANALYZE_BUNDLE=1 yarn web:build",
    "web:no-extract": "NO_EXTRACT=1 yarn web",
    "web:ssr": "PORT=80 node _/web/server.js",
    "web:ssr:development": "NODE_ENV=development yarn web:ssr",
    "ios:bump-version": "cd ios && xcrun agvtool next-version -all",
    "ios:install": "cd ios && arch -x86_64 pod install",
    "ios:run": "TARGET=native arch -x86_64 react-native run-ios",
    "ios": "DISH_ENV=development yarn ios:run  --no-packager --simulator=\"iPhone 11 Pro\"",
    "ios:prod": "DISH_ENV=production yarn ios:run --configuration Release",
    "ios:device": "yarn ios:run --device",
    "ios:device:prod": "yarn ios:run --device --configuration Release",
    "ios:bundle": "TARGET=native react-native bundle --entry-file='index.js' --bundle-output='./ios/dish/main.jsbundle' --dev=false --platform='ios' --assets-dest='./iOS'",
    "android": "TARGET=native DISH_ENV=development react-native run-android --no-packager",
    "dev:react": "npx react-devtools",
    "test": "true",
    "build": "true",
    "build:docker": "cd .. && docker build -t dish/app -f dish-app/Dockerfile .",
    "build:types": "tsc",
    "build:output": "tsc -p tsconfig.build.json",
    "build:splash": "expo-splash-screen --image-resize-mode contain -b '#000000' -i src/assets/splash.png",
    "build:icon": "npx react-native-svg-app-icon",
    "watch": "tsc --watch --preserveWatchOutput",
    "tags:generate": "../dishctl.sh dish_app_generate_tags"
  },
  "dependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@dish/api": "*",
    "@dish/async": "*",
    "@dish/fast-compare": "2.7.6",
    "@dish/graph": "*",
    "@dish/helpers": "*",
    "@dish/helpers-node": "*",
    "@dish/react-feather": "*",
    "@dish/router": "*",
    "@dish/server": "*",
    "@dish/use-store": "*",
    "@dish/webpack": "*",
    "@expo/html-elements": "^0.0.1",
    "@formatjs/intl-datetimeformat": "^3.3.3",
    "@formatjs/intl-displaynames": "^4.0.14",
    "@formatjs/intl-getcanonicallocales": "^1.5.9",
    "@formatjs/intl-locale": "^2.4.23",
    "@formatjs/intl-numberformat": "^6.2.8",
    "@formatjs/intl-pluralrules": "^4.0.16",
    "@formatjs/intl-relativetimeformat": "^8.1.6",
    "@loadable/component": "^5.14.1",
    "@mdx-js/react": "^1.6.22",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-mapbox-gl/maps": "^8.2.0-beta2",
    "@sendgrid/mail": "7.4.0",
    "@sentry/react-native": "^2.4.2",
    "@turf/bbox": "^6.3.0",
    "@turf/center": "^6.3.0",
    "@turf/helpers": "6.3.0",
    "@turf/intersect": "^6.3.0",
    "@turf/union": "^6.3.0",
    "apple-sign-in-rest": "^1.0.2",
    "aws-sdk": "^2.762.0",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.21.1",
    "expo": "^40.0.1",
    "expo-constants": "^10.1.3",
    "expo-linear-gradient": "^9.0.0",
    "expo-status-bar": "^1.0.3",
    "express": "^4.17.1",
    "flexsearch": "^0.6.32",
    "fs-extra": "^9.1.0",
    "gqless": "^2.0.13",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "immer": "^8.0.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "*",
    "mapbox-gl": "^2.2.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "polylabel": "^1.1.0",
    "prop-types": "^15.7.2",
    "react": "*",
    "react-dom": "*",
    "react-error-boundary": "^3.1.0",
    "react-feather": "^2.0.9",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.13.0",
    "react-laag": "^2.0.2",
    "react-native": "^0.64.0",
    "react-native-fast-image": "^8.3.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^2.1.0",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-svg": "^12.1.1",
    "react-query": "^3.12.0",
    "react-reparenting": "^0.6.1",
    "react-storage-hooks": "^4.0.1",
    "react-use": "^15.3.4",
    "recyclerlistview": "^3.0.5",
    "redis": "^3.0.2",
    "resize-observer-polyfill": "^1.5.1",
    "testcafe": "^1.11.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@dish/babel-preset": "*",
    "@snackui/babel-plugin": "*",
    "@types/lodash": "^4.14.165",
    "@types/mapbox-gl": "^2.1.1",
    "@types/react": "*",
    "@types/react-native": "*",
    "@types/redis": "^2.8.28",
    "@types/requestidlecallback": "0.3.1",
    "@unimodules/core": "^7.0.0",
    "@unimodules/react-native-adapter": "^6.1.0",
    "@welldone-software/why-did-you-render": "^6.0.5",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eas-cli": "^0.13.0",
    "expo": "^40.0.1",
    "expo-app-loading": "^1.0.3",
    "expo-asset": "^8.3.1",
    "expo-splash-screen": "^0.10.2",
    "expo-updates": "^0.5.4",
    "expo-yarn-workspaces": "^1.3.1",
    "metro-react-native-babel-preset": "^0.65.1",
    "patch-package": "*",
    "pod-install": "^0.1.18",
    "react-native-animatable": "^1.3.3",
    "react-native-dotenv": "^2.5.3",
    "react-native-unimodules": "^0.12.0",
    "react-test-renderer": "^17.0.1",
    "typescript": "*"
  }
}
