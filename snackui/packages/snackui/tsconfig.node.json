{
  "extends": "./tsconfig",
  "compilerOptions": {
    "composite": false,
    "incremental": false,
    "outDir": "dist",
    "module": "CommonJS",
    "plugins": [
      {
        "transform": "ts-transform-import-path-rewrite",
        "import": "transform",
        "alias": {
          "^(react-native)$": "@dish/proxy-worm"
        },
        "type": "config"
      }
    ]
  }
}
