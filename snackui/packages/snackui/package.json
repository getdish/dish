{
  "name": "snackui",
  "version": "0.0.6",
  "main": "_/index.js",
  "module": "_/index.js",
  "tsmain": "src/index",
  "types": "_/index.d.ts",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "prebuild": "rm -f tsconfig.tsbuildinfo",
    "build": "npm-run-all --parallel build:main build:node",
    "build:main": "tsc -b",
    "build:node": "ttsc -p tsconfig.node.json",
    "watch": "npm-run-all --parallel watch:main watch:web",
    "watch:main": "tsc -b --watch --preserveWatchOutput",
    "watch:web": "ttsc -p tsconfig.node.json --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@snackui/helpers": "^0.0.6",
    "@dish/async": "^0.0.1",
    "@dish/fast-compare": "^2.7.6",
    "@dish/proxy-worm": "^0.0.4",
    "@react-native-community/blur": "^3.6.0",
    "expo-linear-gradient": "^8.4.0",
    "lodash": "^4.17.20",
    "react-feather": "2.0.8",
    "react-laag": "^1.7.3",
    "react-native-web": "*",
    "react-storage-hooks": "^4.0.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@types/node": "^14.11.10",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "npm-run-all": "^4.1.5",
    "ts-transform-import-path-rewrite": "^0.3.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.1.2"
  },
  "gitHead": "503b64ff877fbb47382ba34266af192b2de81ffb"
}
