    "@babel/traverse": "^7.11.5",
    "@babel/types": "^7.11.5",
    "snackui": "*",
    "babel-literal-to-ast": "^2.1.0",
    "fs-extra": "^9.0.0",
    "invariant": "^2.2.4",
    "loader-utils": "^2.0.0",
    "memory-fs": "*",
    "react-native-web": "*",
    "webpack": "^4.44.2"
  },
  "devDependencies": {
    "typescript": "4.1.0-beta",
    "@types/node": "^14.11.10",
    "@o/babel-preset": "*",
    "@o/react-test-env": "*",
    "@types/react-native": "^0.62.12",
    "@types/webpack": "4.41.13",
    "ava": "^3.8.2",
