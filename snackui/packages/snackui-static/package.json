{
  "name": "@snackui/static",
  "version": "0.0.4",
  "main": "_/index.js",
  "scripts": {
    "prebuild": "rm -f tsconfig.tsbuildinfo",
    "build": "tsc -b",
    "watch": "tsc -b --watch --preserveWatchOutput",
    "test": "SNACKUI_COMPILE_PROCESS=1 ava test/* --verbose",
    "test:debug": "SNACKUI_COMPILE_PROCESS=1 DEBUG_FILE=extract-specs.tsx DEBUG=1 yarn test",
    "test:watch": "SNACKUI_COMPILE_PROCESS=1 yarn test --watch",
    "test:update-snapshots": "ava --update-snapshots"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.12.9",
    "@babel/generator": "^7.12.5",
    "@babel/helper-plugin-utils": "^7.10.4",
    "@babel/parser": "^7.12.7",
    "@babel/traverse": "^7.12.9",
    "@babel/types": "^7.12.7",
    "babel-literal-to-ast": "^2.1.0",
    "fs-extra": "^9.0.0",
    "invariant": "^2.2.4",
    "loader-utils": "^2.0.0",
    "memfs": "^3.2.0",
    "react-native-web": "*",
    "snackui": "^0.0.4",
    "unionfs": "^4.4.0",
    "webpack": "^5.10.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-typescript": "^7.12.1",
    "@dish/babel-preset": "*",
    "@dish/react-test-env": "*",
