    "css-loader": "^3.5.3",
    "esm": "^3.2.25",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.0.0",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "mutationobserver-polyfill": "1.3.0",
    "react": "*",
    "react-dom": "*",
    "react-test-renderer": "^16.14.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.1.2"
