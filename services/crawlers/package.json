{
  "name": "@dish/crawlers",
  "version": "0.0.1",
  "main": "_/index.js",
  "sideEffects": false,
  "scripts": {
    "start": "node _/index.js",
    "build": "tsc -b",
    "watch": "tsc -b --watch --preserveWatchOutput",
    "worker": "node _/worker.js",
    "worker:watch": "nodemon _/worker.js",
    "test": "TARGET=node RUN_WITHOUT_WORKER=true DISH_ENV=test ava test/idempotent/* --verbose",
    "test:scheduled": "TARGET=node RUN_WITHOUT_WORKER=true DISH_ENV=test ava test/http-dependent/* --verbose"
  },
  "dependencies": {
    "@dish/common": "*",
    "@dish/graph": "*",
    "@dish/worker": "*",
    "@types/puppeteer": "^3.0.0",
    "acorn": "^7.1.1",
    "aws-sdk": "^2.719.0",
    "axios": "^0.19.1",
    "sinon": "^9.0.3",
    "bull": "^3.12.1",
    "cheerio": "^1.0.0-rc.3",
    "chrono-node": "^2.1.6",
    "csv-parse": "^4.9.0",
    "js-base64": "^2.5.2",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "pg": "^7.18.2",
    "proxy-chain": "^0.4.4",
    "puppeteer": "^4.0.0",
    "puppeteer-extra": "^3.1.9",
    "puppeteer-extra-plugin-stealth": "^2.4.9",
    "sentiment": "^5.0.2",
    "tabletojson": "^2.0.4",
    "transliteration": "^2.1.8"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.16",
    "@types/lodash": "^4.14.149",
    "@types/puppeteer": "^3.0.1",
    "@zeit/ncc": "^0.20.5",
    "ava": "^3.8.2",
    "bull-repl": "^0.26.0",
    "nodemon": "^2.0.2",
    "typescript": "*"
  }
}
