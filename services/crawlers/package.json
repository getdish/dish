{
  "name": "@dish/crawlers",
  "version": "0.0.1",
  "source": "src/index.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "sideEffects": false,
  "testConfig": {
    "isolate": [
      "test:idempotent",
      "test:http"
    ]
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "crawl:one": "SLUG=$SLUG node -r esbuild-register ./scripts/crawl_one.ts",
    "test": "DISH_DEBUG=1 ./scripts/run_tests.sh",
    "test:idempotent": "ava test/idempotent/* --verbose --serial",
    "test:http": "ava test/http-dependent/* --verbose --serial",
    "test:http:one": "ava test/http-dependent/$SERVICE.ts --verbose --serial"
  },
  "dependencies": {
    "@dish/async": "*",
    "@dish/common": "*",
    "@dish/graph": "*",
    "@dish/helpers": "*",
    "@dish/helpers-node": "*",
    "@dish/worker": "*",
    "acorn": "^7.1.1",
    "ava": "^3.15.0",
    "aws-sdk": "^2.762.0",
    "axios": "^0.19.2",
    "bull": "^3.22.8",
    "cheerio": "^1.0.0-rc.5",
    "chrono-node": "^2.1.6",
    "csv-parse": "^4.9.0",
    "fast-levenshtein": "^3.0.0",
    "form-data": "^4.0.0",
    "gqty": "2.0.2",
    "graphql": "^15.5.0",
    "imghash": "^0.0.9",
    "js-base64": "^2.5.2",
    "lodash": "*",
    "moment": "^2.29.1",
    "node-fetch": "^3.0.0",
    "pg": "^8.7.1",
    "proxy-chain": "^0.4.4",
    "puppeteer-core": "^9.0.0",
    "puppeteer-extra": "^3.1.18",
    "puppeteer-extra-plugin-stealth": "^2.7.6",
    "react": "*",
    "sentiment": "^5.0.2",
    "sinon": "^9.0.3",
    "tabletojson": "^2.0.4",
    "transliteration": "^2.1.8"
  },
  "devDependencies": {
    "@tamagui/build": "^1.0.0-alpha.32",
    "@types/cheerio": "^0.22.24",
    "@types/pg": "^7.14.10",
    "@types/puppeteer": "^5.4.3",
    "typescript": "*"
  }
}
