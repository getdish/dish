{
  "name": "@dish/crawlers",
  "version": "0.0.1",
  "main": "_/index.js",
  "sideEffects": false,
  "scripts": {
    "start": "node _/index.js",
    "build": "tsc -b",
    "watch": "tsc -b --watch --preserveWatchOutput",
    "worker": "node _/worker.js",
    "worker:watch": "nodemon _/worker.js",
    "test": "RUN_WITHOUT_WORKER=true ava test/idempotent/* --verbose",
    "test:scheduled": "RUN_WITHOUT_WORKER=true ava test/http-dependent/* --verbose"
  },
  "dependencies": {
    "@dish/common": "*",
    "@dish/models": "*",
    "@dish/worker": "*",
    "acorn": "^7.1.1",
    "axios": "^0.19.1",
    "cheerio": "^1.0.0-rc.3",
    "js-base64": "^2.5.2",
    "lodash": "^4.17.15",
    "pg": "^7.18.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.16",
    "@types/lodash": "^4.14.149",
    "@zeit/ncc": "^0.20.5",
    "nodemon": "^2.0.2",
    "typescript": "3.7.3"
  }
}
