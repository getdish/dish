{
  "name": "@dish/crawlers",
  "version": "0.0.1",
  "types": "types.d.ts",
  "main": "dist",
  "sideEffects": false,
  "testConfig": {
    "isolate": [
      "test:idempotent",
      "test:http"
    ]
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "esdx",
    "watch": "esdx-watch",
    "crawl:one": "ENV=dev ./scripts/run_crawl.sh --one",
    "crawl:one:internal": "ENV=dev ./scripts/run_crawl.sh --one-internal",
    "test": "DISH_DEBUG=1 ./scripts/run_tests.sh",
    "test:idempotent": "ava test/idempotent/* --verbose --serial",
    "test:http": "ava test/http-dependent/* --verbose --serial"
  },
  "dependencies": {
    "@dish/async": "*",
    "@dish/common": "*",
    "@dish/esdx": "*",
    "@dish/graph": "*",
    "@dish/helpers": "*",
    "@dish/worker": "*",
    "acorn": "^7.1.1",
    "ava": "^3.14.0",
    "aws-sdk": "^2.762.0",
    "axios": "^0.19.1",
    "bull": "^3.18.0",
    "cheerio": "^1.0.0-rc.5",
    "chrono-node": "^2.1.6",
    "csv-parse": "^4.9.0",
    "fast-levenshtein": "^3.0.0",
    "form-data": "^4.0.0",
    "gqless": "^2.0.13",
    "graphql": "^15.5.0",
    "imghash": "^0.0.9",
    "js-base64": "^2.5.2",
    "lodash": "*",
    "moment": "^2.27.0",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "proxy-chain": "^0.4.4",
    "puppeteer-core": "^9.0.0",
    "puppeteer-extra": "^3.1.18",
    "puppeteer-extra-plugin-stealth": "^2.7.6",
    "react": "^17.0.1",
    "sentiment": "^5.0.2",
    "sinon": "^9.0.3",
    "tabletojson": "^2.0.4",
    "transliteration": "^2.1.8"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.24",
    "@types/pg": "^7.14.10",
    "@types/puppeteer": "^5.4.3",
    "typescript": "*"
  }
}
