{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "preserveSymlinks": true,
    "incremental": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "target": "ES2019"
  }
}
