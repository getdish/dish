{
  "extends": "../tsconfig.base",
  "compilerOptions": {
    "baseUrl": "..",
    "paths": {
      "@dish/*": ["packages/*"]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "noFallthroughCasesInSwitch": true,
    "emitDeclarationOnly": false,
    "noEmit": true,
    "noEmitHelpers": true,
    "noImplicitReturns": false,
    "target": "es2019",
    "strict": false,
    "strictNullChecks": true,
    "removeComments": true,
    "typeRoots": [
      "node_modules/@types",
      "./types",
      "../../node_modules/@types"
    ]
  },
  "include": [
    "src",
    "declaration.d.ts"
  ],
  "exclude": [
    "_"
  ],
  "references": [
    {
      "path": "../packages/api"
    },
    {
      "path": "../packages/async"
    },
    {
      "path": "../packages/fast-compare"
    },
    {
      "path": "../packages/graph"
    },
    {
      "path": "../packages/helpers"
    },
    {
      "path": "../packages/helpers-node"
    },
    {
      "path": "../packages/router"
    },
    {
      "path": "../packages/server"
    },
    {
      "path": "../packages/use-store"
    },
    {
      "path": "../packages/ui"
    },
    {
      "path": "../packages/webpack"
    }
  ]
}
