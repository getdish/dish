{
  "name": "@dish/app",
  "private": true,
  "version": "0.0.0",
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "clean": "react-native-clean-project",
    "etc:colorize": "../dsh run 'DEBUG=1 node -r esbuild-register ./etc/add_colors_to_tiles.ts'",
    "postinstall": "./etc/postinstall.sh",
    "etc:colorize:staging": "NODE_ENV=staging yarn etc:colorize",
    "app:start": "BABEL_ENV=production DISH_DEBUG=1 LOCAL_HOST=$(ipconfig getifaddr en0) LOG_LEVEL=1 TARGET=native react-native start",
    "app": "DISH_ENV=development dsh run 'yarn app:start'",
    "app:prod": "DISH_ENV=production dsh run 'yarn app:start --reset-cache'",
    "app:prod:dev": "DISH_ENV=development IS_LIVE=1 dsh run 'yarn app:start --reset-cache'",
    "app:build": "GIT_COMMIT=$(git rev-parse --short HEAD) npx eas build --platform ios --non-interactive",
    "app:reset": "yarn app --reset-cache",
    "app:reset:hard": "watchman watch-del-all && yarn app:reset",
    "web": "IS_LOCAL=1 DISH_ENV=development ../dsh run 'yarn dish start --local-proxy'",
    "web:reset": "NODE_ENV=development ../dsh run 'yarn dish start --reset-cache'",
    "web:https": "sudo dish start --https --host d1sh.com --port 443",
    "web:debug": "DEBUG_WEBPACK=1 node --max_old_space_size=8192 --inspect ../node_modules/.bin/dish start --serial",
    "web:prod": "DISH_ENV=production ../dsh run 'yarn dish start --prod'",
    "web:prod:reset": "DISH_ENV=production ../dsh run 'yarn dish start --prod --reset-cache'",
    "web:prod:dev": "DISH_ENV=production ../dsh run 'yarn dish start --prod --no-optimize'",
    "web:serve": "dish serve",
    "web:build": "GIT_COMMIT=$(git rev-parse --short HEAD) NODE_ENV=production dish build",
    "web:build:analyze": "ANALYZE_BUNDLE=1 yarn web:build",
    "web:no-extract": "NO_EXTRACT=1 yarn web",
    "web:ssr": "PORT=80 node _/web/server.js",
    "web:ssr:development": "NODE_ENV=development yarn web:ssr",
    "ios:bump-version": "cd ios && xcrun agvtool next-version -all",
    "ios:install": "cd ios && USE_HERMES=1 arch -x86_64 pod install --repo-update",
    "ios:run": "TARGET=native arch -x86_64 react-native run-ios",
    "ios": "DISH_ENV=development yarn ios:run  --no-packager --simulator=\"iPhone 11 Pro\"",
    "ios:prod": "DISH_ENV=production yarn ios:run --configuration Release",
    "ios:device": "yarn ios:run --device",
    "ios:device:prod": "yarn ios:run --device --configuration Release",
    "ios:bundle": "TARGET=native react-native bundle --entry-file='index.js' --bundle-output='./ios/dish/main.jsbundle' --dev=false --platform='ios' --assets-dest='./iOS'",
    "ios:bundle:prod": "TARGET=native react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "android": "TARGET=native DISH_ENV=development react-native run-android --no-packager",
    "dev:react": "npx react-devtools",
    "test": "true",
    "build": "true",
    "build:eas": "EXPO_NO_CAPABILITY_SYNC=1 yarn eas build",
    "build:docker": "cd .. && docker build -t dish/app -f app/Dockerfile .",
    "build:types": "tsc",
    "build:output": "tsc -p tsconfig.build.json",
    "build:splash": "expo-splash-screen --image-resize-mode contain -b '#000000' -i src/assets/splash.png",
    "build:icon": "npx react-native-svg-app-icon",
    "watch": "tsc --watch --preserveWatchOutput",
    "tags:generate": "../dsh dish_app_generate_tags"
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@dish/api": "*",
    "@dish/async": "*",
    "@dish/fast-compare": "2.7.6",
    "@dish/graph": "*",
    "@dish/helpers": "*",
    "@dish/helpers-node": "*",
    "@dish/react-feather": "*",
    "@dish/router": "*",
    "@dish/server": "*",
    "@dish/use-store": "*",
    "@dish/webpack": "*",
    "@expo/html-elements": "^0.0.1",
    "@formatjs/intl-datetimeformat": "^3.3.3",
    "@formatjs/intl-displaynames": "^4.0.14",
    "@formatjs/intl-getcanonicallocales": "^1.5.9",
    "@formatjs/intl-locale": "^2.4.23",
    "@formatjs/intl-numberformat": "^6.2.8",
    "@formatjs/intl-pluralrules": "^4.0.16",
    "@formatjs/intl-relativetimeformat": "^8.1.6",
    "@loadable/component": "^5.14.1",
    "@mdx-js/react": "^1.6.22",
    "@nandorojo/anchor": "^0.2.2",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-mapbox-gl/maps": "^8.3.0",
    "@sendgrid/mail": "7.4.0",
    "@sentry/browser": "^6.12.0",
    "@sentry/react-native": "^2.6.2",
    "@sentry/tracing": "^6.12.0",
    "@turf/bbox": "^6.3.0",
    "@turf/center": "^6.3.0",
    "@turf/helpers": "6.3.0",
    "@turf/intersect": "^6.3.0",
    "@turf/union": "^6.3.0",
    "apple-sign-in-rest": "^1.0.2",
    "aws-sdk": "^2.762.0",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.21.1",
    "expo": "^42.0.3",
    "expo-image-picker": "^10.2.3",
    "express": "^4.17.1",
    "flexsearch": "^0.6.32",
    "fs-extra": "^9.1.0",
    "gqty": "2.0.2-experimental.1",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "immer": "^8.0.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "*",
    "mapbox-gl": "^2.4.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "polylabel": "^1.1.0",
    "prop-types": "^15.7.2",
    "react": "*",
    "react-dom": "*",
    "react-error-boundary": "^3.1.0",
    "react-feather": "^2.0.9",
    "react-globe.gl": "2.18.10",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.13.0",
    "react-laag": "2.0.3",
    "react-lag-radar": "^1.0.0",
    "react-native": "^0.63.4",
    "react-native-draggable-flatlist": "^2.6.2",
    "react-native-fast-image": "^8.5.8",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^2.2.0",
    "react-native-screens": "^3.7.2",
    "react-native-svg": "^12.1.1",
    "react-query": "^3.23.0",
    "react-reparenting": "^0.6.1",
    "recyclerlistview": "^3.0.5",
    "redis": "^3.1.2",
    "smoothscroll-polyfill": "^0.4.4",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@dish/babel-plugin-inline-env": "*",
    "@dish/babel-preset": "*",
    "@snackui/babel-plugin": "*",
    "@types/lodash": "^4.14.165",
    "@types/mapbox-gl": "^2.4.0",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.10",
    "@types/react": "*",
    "@types/react-native": "*",
    "@types/redis": "^2.8.28",
    "@types/requestidlecallback": "0.3.1",
    "@types/uuid": "^8.3.1",
    "@unimodules/core": "^7.0.0",
    "@unimodules/react-native-adapter": "^6.1.0",
    "@welldone-software/why-did-you-render": "^6.1.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eas-cli": "^0.28.2",
    "expo": "^42.0.3",
    "expo-app-loading": "^1.1.2",
    "expo-linear-gradient": "^9.2.0",
    "expo-splash-screen": "^0.11.2",
    "expo-yarn-workspaces": "^1.5.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "patch-package": "*",
    "pod-install": "^0.1.18",
    "react-native-animatable": "^1.3.3",
    "react-native-clean-project": "^3.6.4",
    "react-native-codegen": "^0.0.7",
    "react-native-dotenv": "^2.5.3",
    "react-native-unimodules": "^0.14.6",
    "react-test-renderer": "^17.0.1",
    "testcafe": "^1.11.0",
    "thread-loader": "^3.0.3",
    "typescript": "*"
  }
}
