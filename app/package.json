{
  "name": "@dish/app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "clean": "react-native-clean-project",
    "postinstall": "./scripts/postinstall.sh",
    "scripts:crawl:one": "SLUG=$SLUG ../dsh run 'node -r esbuild-register ./scripts/crawl_one.ts'",
    "scripts:import_geojson_tiles": "../dsh run 'node -r esbuild-register ./scripts/import_geojson_tiles.ts'",
    "scripts:import_geojson_tiles:state": "FILE=state.geo.json DEST=boundaries-state yarn scripts:import_geojson_tiles",
    "scripts:import_geojson_tiles:city": "FILE=city.geo.json DEST=boundaries-city yarn scripts:import_geojson_tiles",
    "scripts:colorize": "DEBUG=1  ../dsh run 'node -r esbuild-register ./scripts/add_colors_to_tiles.ts'",
    "scripts:colorize:staging": "NODE_ENV=staging yarn scripts:colorize",
    "worker:start": "node dist/index.js",
    "worker:start:clear": "CLEAR_JOBS=1 node dist/index.js",
    "worker:repl": "npx bull-repl connect",
    "app:link": "react-native link",
    "app:start": "DISH_DEBUG=1 LOCAL_HOST=$(ipconfig getifaddr en0) LOG_LEVEL=1 TAMAGUI_TARGET=native expo start",
    "app": "DISH_ENV=development dsh run 'yarn app:start'",
    "app:rnx": "DISH_ENV=development dsh run 'yarn app:start:rnx'",
    "app:webpack": "DISH_ENV=development dsh run 'yarn app:start:webpack'",
    "app:prod": "DISH_ENV=production dsh run 'yarn app:start --reset-cache'",
    "app:live": "DISH_ENV=development IS_LIVE=1 dsh run 'yarn app:start --reset-cache'",
    "app:live:prod": "DISH_ENV=production IS_LIVE=1 dsh run 'yarn app:start --reset-cache'",
    "app:build": "GIT_COMMIT=$(git rev-parse --short HEAD) npx eas build --platform ios --non-interactive",
    "app:reset": "yarn app --reset-cache",
    "app:reset:hard": "watchman watch-del-all && yarn app:reset",
    "web": "IS_LOCAL=1 DISH_ENV=development ../dsh run 'yarn dish start --local-proxy'",
    "web:reset": "NODE_ENV=development ../dsh run 'yarn dish start --reset-cache'",
    "web:debug": "DEBUG_WEBPACK=1 node --max_old_space_size=8192 --inspect ../node_modules/.bin/dish start --serial",
    "web:prod": "DISH_ENV=production ../dsh run 'yarn dish start --prod'",
    "web:prod:reset": "DISH_ENV=production ../dsh run 'yarn dish start --prod --reset-cache'",
    "web:prod:dev": "DISH_ENV=production ../dsh run 'yarn dish start --prod --no-optimize'",
    "web:serve": "dish serve",
    "web:build": "GIT_COMMIT=$(git rev-parse --short HEAD) NODE_ENV=production dish build",
    "web:build:analyze": "ANALYZE_BUNDLE=1 yarn web:build",
    "web:no-extract": "NO_EXTRACT=1 yarn web",
    "ios:bump-version": "cd ios && xcrun agvtool next-version -all",
    "ios:install": "cd ios && pod install --repo-update",
    "ios:run": "TAMAGUI_TARGET=native expo ios",
    "ios": "yarn eas build -p ios -e development --local --no-wait --non-interactive",
    "ios:prod": "DISH_ENV=production yarn ios:run --configuration Release",
    "ios:device": "yarn ios:run --device",
    "ios:device:prod": "yarn ios:run --device --configuration Release",
    "ios:bundle": "TAMAGUI_TARGET=native react-native bundle --entry-file='index.js' --bundle-output='./ios/dish/main.jsbundle' --dev=false --platform='ios' --assets-dest='./iOS'",
    "ios:bundle:prod": "TAMAGUI_TARGET=native react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "android": "TAMAGUI_TARGET=native DISH_ENV=development react-native run-android --no-packager",
    "dev:react": "npx react-devtools",
    "test": "../dsh run yarn run ava test/specs/* --verbose --serial",
    "test:idempotent": "ava test/idempotent/* --verbose --serial",
    "test:http": "ava test/http-dependent/* --verbose --serial",
    "test:http:one": "ava test/http-dependent/$SERVICE.ts --verbose --serial",
    "bundle": "react-native rnx-bundle --dev false",
    "build": "true",
    "build:eas": "EXPO_NO_CAPABILITY_SYNC=1 yarn eas build",
    "build:docker": "cd .. && docker build -t dish/app -f app/Dockerfile .",
    "build:types": "tsc",
    "build:output": "tsc -p tsconfig.build.json",
    "build:splash": "expo-splash-screen --image-resize-mode contain -b '#000000' -i src/assets/splash.png",
    "build:icon": "npx react-native-svg-app-icon",
    "watch": "tsc --watch --preserveWatchOutput --noEmit",
    "tags:generate": "../dsh dish_app_generate_tags",
    "start": "expo start --dev-client"
  },
  "rnx-kit": {
    "bundle": {
      "entryPath": "./index.js",
      "detectCyclicDependencies": true,
      "detectDuplicateDependencies": false,
      "bundlePrefix": "dish",
      "targets": [
        "ios"
      ],
      "platforms": {
        "ios": {
          "sourceMapPath": "dish.ios.map"
        }
      }
    }
  },
  "testConfig": {
    "isolate": [
      "test:idempotent",
      "test:http"
    ]
  },
