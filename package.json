{
  "name": "dish-monorepo",
  "private": true,
  "engines": {
    "node": ">=12 <=17"
  },
  "volta": {
    "node": "16.13.2"
  },
  "workspaces": {
    "packages": [
      "app",
      "apps/*",
      "packages/*",
      "services/*"
    ]
  },
  "scripts": {
    "switch:rn": "node ./bin/switch.js",
    "trim-package": "yes | npx json-filter-cli --from=package.json --to=package.json --exclude=scripts",
    "trim-packages": "yarn trim-package && npx yarn workspaces foreach run yes | npx json-filter-cli --confirm --from=package.json --to=package.json --exclude=scripts",
    "bootstrap": "docker-compose build base",
    "upgrade:tamagui": "manypkg upgrade tamagui && manypkg upgrade @tamagui && manypkg upgrade tamagui-loader",
    "link:tamagui": "yarn link ~/tamagui --all",
    "dsh": "node -r esbuild-register dsh.ts",
    "d": "yarn dev:local",
    "dp": "yarn dev:local:prod",
    "dev": "dsh dev",
    "dev:local": "IS_LOCAL=1 dsh dev --local --skip-native",
    "dev:local:prod": "DISH_ENV=production IS_LOCAL=1 dsh dev --local --skip-native",
    "dev:prod": "dsh dev --prod",
    "dev:backend": "dsh dev --backend",
    "dev:web": "yarn web",
    "dev:native": "yarn app",
    "start": "DISH_ENV=${DISH_ENV:-development} ./dsh compose_up",
    "start:test": "COMPOSE_EXCLUDE_EXTRA=\"|app\" DISH_ENV=test ./dsh compose_up",
    "app": "DISH_ENV=development dsh run native",
    "app:live": "cd app && dum app:live",
    "web": "cd app && dum web",
    "web:prod": "cd app && dum web:prod",
    "puppet-proxy": "./dsh run 'yarn workspace @dish/puppet-proxy start'",
    "ios": "cd app && dum ios",
    "ios:install": "cd app && dum ios:install",
    "postinstall": "manypkg check && ./bin/postinstall.sh",
    "preinstall": "manypkg fix",
    "docker:start:web": "yarn stop:docker:web && yarn docker:start:web:simple & sleep 2 && yarn logs:docker:web",
    "docker:start:web:simple": "docker run --name app -it -d -p 4444:4444 --rm dish/app",
    "docker:logs:web": "docker logs -f app",
    "docker:stop:web": "docker rm -f app || true",
    "validate:tsc": "tsc --noEmit",
    "check:ts": "tsc -b --preserveWatchOutput tsconfig.build.json",
    "watch:ts:check": "tsc -w --preserveWatchOutput --noEmit --project tsconfig.build.json",
    "watch:ts": "SKIP_TYPES_INITIAL=1 ultra -r --no-pretty --concurrency 100 watch",
    "build:ordered": "ultra -r --no-pretty --build",
    "build": "ultra -r --no-pretty --concurrency 30 build",
    "build:force": "ultra --silent -r --no-pretty --rebuild --concurrency 30 build",
    "build:js": "JS_ONLY=1 yarn build",
    "build:docker": "./dsh buildkit_build_local",
    "watch": "npm-run-all --parallel watch:ts:check watch:ts",
    "build:refs": "workspaces-to-typescript-project-references",
    "test": "KEEP_ALIVE=0 DISH_ENV=test ./dsh run_dsh",
    "test:dev": "IS_LOCAL=1 yarn test",
    "migrate": "./dsh migrate_all",
    "test:migrate": "DISH_ENV=test ./dsh migrate_all",
    "crawl:tucson": "node services/crawlers/dist/yelp/all.js --city 'Tucson, Arizona'",
    "crawl:osm": "DISH_ENV=development IS_LOCAL=1 DISH_DEBUG=3 ./dsh run 'node -r esbuild-register services/crawlers/scripts/crawl_osm.ts'",
    "crawl:osm:kailua": "LAT=21.38942705899535 LNG=-157.7446711063385 SPAN=0.01 yarn crawl:osm",
    "crawl:one": "DISH_ENV=test IS_LOCAL=1 DISH_DEBUG=3 SLUG=$SLUG ./dsh run 'node -r esbuild-register services/crawlers/scripts/crawl_one.ts'",
    "crawl:one:production": "IS_LIVE=1 DEBUG=1 DISH_ENV=production SLUG=$SLUG yarn crawl:one",
    "crawl:one:external": "SKIP=internal yarn crawl:one",
    "crawl:one:yelp": "SKIP=internal,google,tripadvisor yarn crawl:one",
    "crawl:internal": "./dsh run 'RUN=1 QUERY=\"SELECT id from restaurant $QUERY\" node ./services/crawlers/dist/self/one.js'",
    "crawl:internal:slug": "QUERY=\"WHERE slug = '$SLUG' LIMIT 1\" yarn crawl:internal",
    "graph:generate": "yarn workspace @dish/graph generate",
    "clean": "./dsh clean",
    "clean:build": "./dsh clean_build",
    "hasura:migrate": "./dsh migrate_hasura",
    "hasura:migrate:test": "DISH_ENV=test yarn hasura:migrate",
    "hasura:migrate:create": "./dsh hasura_cmd migrate create",
    "hasura:admin": "./dsh hasura_admin",
    "prettier": "prettier --write \"**/*.(ts,tsx)\" || true",
    "timescale:psql": "psql -d postgres -U postgres -p 5432 -h localhost"
  },
  "resolutions": {
    "jest-haste-map": "28.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "@turf/helpers": "6.5.0",
    "domelementtype": "2.3.0",
    "fbjs": "3.0.4",
    "@sentry/utils": "6.19.7",
    "@sentry/types": "6.19.7",
    "prop-types": "15.8.1",
    "react-native-svg": "12.1.1",
    "expo-modules-autolinking": "0.8.0",
    "expo-screen-orientation": "4.2.0",
    "react-native-reanimated": "3.0.0-rc.0",
    "react-native-gesture-handler": "2.4.2",
    "metro": "0.71.0",
    "@types/react-native": "0.66.6",
    "@types/react": "17.0.19",
    "@testing-library/react": "13.3.0",
    "fs-extra": "10.1.0",
    "@react-native-community/cli": "7.0.2",
    "react-native": "0.68.1",
    "typescript": "4.8.0-dev.20220731",
    "aws-sdk": "2.762.0",
    "lodash": "4.17.21",
    "resolve": "1.17.0",
    "tslib": "2.4.0",
    "esbuild-register": "3.3.2",
    "@types/puppeteer": "3.0.1",
    "esbuild": "0.14.50",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-decorators": "7.15.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.14.5",
    "@babel/plugin-proposal-optional-chaining": "7.14.5",
    "@babel/plugin-transform-react-constant-elements": "7.14.5",
    "@babel/plugin-transform-react-inline-elements": "7.14.5",
    "@babel/plugin-transform-react-jsx": "7.14.9",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@babel/core": "7.15.5",
    "@babel/generator": "7.15.4",
    "@babel/parser": "7.15.7",
    "@babel/traverse": "7.15.4",
    "@babel/types": "7.15.6",
    "react-native-web": "0.18.7"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.16",
    "@dish/app": "*",
    "@manypkg/cli": "^0.19.1",
    "@monorepo-utils/workspaces-to-typescript-project-references": "^2.7.4",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/apple-mapkit-js": "^5.50.0",
    "@types/express-http-proxy": "^1.6.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.10",
    "@types/react": "*",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@yarnpkg/plugin-patch": "^2.1.2",
    "ava": "^3.15.0",
    "esbuild": "^0.14.38",
    "esbuild-jest": "^0.5.0",
    "esbuild-register": "^2.6.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-react": "^7.24.0",
    "esm": "^3.2.25",
    "fs-extra": "^10.1.0",
    "identity-obj-proxy": "^3.0.0",
    "import-sort-style-module": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "prettier": "^2.6.2",
    "react-native": "^0.68.1",
    "react-native-web": "^0.18.7",
    "react-test-renderer": "18.0.0-beta-149b420f6-20211119",
    "typescript": "*",
    "ultra-runner": "^3.10.5",
    "zx": "^1.14.1"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "version": "0.0.0",
  "packageManager": "yarn@3.1.1"
}
