{
  "name": "dish-monorepo",
  "private": true,
  "changelog": {
    "repo": "snackui/snackui"
  },
  "engines": {
    "node": ">=12 <=15"
  },
  "workspaces": {
    "packages": [
      "app",
      "packages/*",
      "services/*",
      "snackui/packages/*"
    ]
  },
  "scripts": {
    "switch:rn": "node ./bin/switch.js",
    "trim-package": "yes | npx json-filter-cli --from=package.json --to=package.json --exclude=scripts",
    "trim-packages": "yarn trim-package && npx yarn workspaces foreach run yes | npx json-filter-cli --confirm --from=package.json --to=package.json --exclude=scripts",
    "bootstrap": "docker-compose build base",
    "start": "DISH_ENV=development ./dsh compose_up",
    "start:test": "COMPOSE_EXCLUDE_EXTRA=\"|app\" DISH_ENV=test ./dsh compose_up",
    "site": "yarn workspace @dish/site start",
    "app": "yarn workspace @dish/app app",
    "web": "yarn workspace @dish/app web",
    "puppet-proxy": "./dsh run 'yarn workspace @dish/puppet-proxy start'",
    "etc:colorize": "yarn workspace @dish/app etc:colorize",
    "ios": "yarn workspace @dish/app ios",
    "ios:install": "yarn workspace @dish/app ios:install",
    "postinstall": "./bin/postinstall.sh",
    "preinstall": "./bin/preinstall.sh",
    "dsh": "./dsh run_dsh",
    "docker:start:web": "yarn stop:docker:web && yarn docker:start:web:simple & sleep 2 && yarn logs:docker:web",
    "docker:start:web:simple": "docker run --name app -it -d -p 4444:4444 --rm dish/app",
    "docker:logs:web": "docker logs -f app",
    "docker:stop:web": "docker rm -f app || true",
    "validate:tsc": "tsc --noEmit",
    "check:ts": "tsc -b --preserveWatchOutput tsconfig.build.json",
    "check:ts:watch": "tsc -w --preserveWatchOutput --noEmit --project tsconfig.build.json",
    "build:ts:watch": "ultra -r --no-pretty --concurrency 100 watch",
    "build:ordered": "ultra -r --no-pretty --build",
    "build": "ultra -r --no-pretty --concurrency 30 build",
    "build:force": "ultra --silent -r --no-pretty --rebuild --concurrency 30 build",
    "build:js": "JS_ONLY=1 yarn build",
    "build:docker": "./dsh buildkit_build_local",
    "watch": "npm-run-all --parallel check:ts:watch build:ts:watch",
    "watch:js": "JS_ONLY=1 yarn build:ts:watch",
    "build:refs": "typescript-monorepo-toolkit inject-refs .",
    "test": "KEEP_ALIVE=0 DISH_ENV=test ./dsh run_dsh",
    "test:dev": "IS_LOCAL=1 yarn test",
    "test:migrate": "DISH_ENV=test ./dsh migrate_all",
    "crawl:tucson": "node services/crawlers/dist/yelp/all.js --city 'Tucson, Arizona'",
    "crawl:one": "IS_LOCAL=1 DISH_DEBUG=3 SLUG=$SLUG ./dsh run 'node services/crawlers/dist/one.js'",
    "crawl:one:production": "IS_LIVE=1 DEBUG=1 DISH_ENV=production SLUG=$SLUG yarn crawl:one",
    "crawl:one:external": "SKIP=internal yarn crawl:one",
    "crawl:one:yelp": "SKIP=internal,google,tripadvisor yarn crawl:one",
    "crawl:internal": "./dsh run 'RUN=1 QUERY=\"SELECT id from restaurant $QUERY\" node ./services/crawlers/dist/self/one.js'",
    "crawl:internal:slug": "QUERY=\"WHERE slug = '$SLUG' LIMIT 1\" yarn crawl:internal",
    "graph:generate": "yarn workspace @dish/graph generate",
    "clean": "./dsh clean",
    "clean:build": "./dsh clean_build",
    "hasura:migrate": "./dsh migrate_hasura",
    "hasura:migrate:test": "DISH_ENV=test yarn hasura:migrate",
    "hasura:migrate:create": "./dsh hasura_cmd migrate create",
    "hasura:admin": "./dsh hasura_admin",
    "prettier": "prettier --write \"**/*.(ts,tsx)\" || true",
    "expo:check-deps": "expo-yarn-workspaces check-workspace-dependencies",
    "timescale:psql": "psql -d postgres -U postgres -p 5432 -h localhost",
    "snackui:pull": "git subrepo pull snackui",
    "snackui:push": "git subrepo clean snackui && git subrepo push snackui"
  },
  "resolutions": {
    "expo-screen-orientation": "3.3.0",
    "react-native-reanimated": "https://github.com/software-mansion/react-native-reanimated.git#a4f4c83729899b6f77812294e771deae05b48b9c",
    "metro": "0.66.2",
    "metro-babel-register": "0.66.2",
    "metro-cache": "0.66.2",
    "metro-config": "0.66.2",
    "metro-core": "0.66.2",
    "metro-react-native-babel-preset": "0.66.2",
    "metro-react-native-babel-transformer": "0.66.2",
    "metro-resolver": "0.66.2",
    "metro-source-map": "0.66.2",
    "metro-symbolicate": "0.66.2",
    "@types/react": "17.0.19",
    "react": "0.0.0-experimental-b1a1cb116-20210924",
    "react-dom": "0.0.0-experimental-b1a1cb116-20210924",
    "react-fetch": "0.0.0-experimental-b1a1cb116-20210924",
    "react-refresh": "0.0.0-experimental-b1a1cb116-20210924",
    "react-test-renderer": "0.0.0-experimental-b1a1cb116-20210924",
    "@testing-library/react": "13.0.0-alpha.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.1",
    "fs-extra": "10.0.0",
    "react-native": "0.66.0-rc.4",
    "typescript": "4.4.3",
    "@unimodules/react-native-adapter": "6.3.9",
    "@unimodules/core": "7.1.2",
    "aws-sdk": "2.762.0",
    "webpack": "5.55.1",
    "lodash": "4.17.20",
    "resolve": "1.17.0",
    "jsdom": "16.4.0",
    "tslib": "2.3.0",
    "expo-yarn-workspaces": "1.3.1",
    "esbuild-register": "2.6.0",
    "@types/puppeteer": "3.0.1",
    "esbuild": "0.12.28",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-react-constant-elements": "^7.14.5",
    "@babel/plugin-transform-react-inline-elements": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/core": "^7.15.5",
    "@babel/generator": "^7.15.4",
    "@babel/parser": "^7.15.7",
    "@babel/traverse": "^7.15.4",
    "@babel/types": "^7.15.6",
    "expo-linear-gradient": "9.2.0",
    "react-native-unimodules": "0.14.8",
    "react-native-web": "0.16.5"
  },
  "dependencies": {
    "@dish/app": "*",
    "@snackui/node": "*",
    "@yarnpkg/plugin-patch": "^2.1.2",
    "esbuild-register": "^2.6.0",
    "fs-extra": "^10.0.0",
    "identity-obj-proxy": "^3.0.0",
    "node-fetch": "3.0.0-beta.9",
    "react-native-web": "^0.16.5",
    "react-refresh": "*",
    "snackui": "*",
    "tsconfig-paths": "^3.9.0",
    "typescript": "*",
    "zx": "^1.14.1"
  },
  "devDependencies": {
    "@babel/runtime": "^7.13.16",
    "@types/apple-mapkit-js": "^5.50.0",
    "@types/express-http-proxy": "^1.6.1",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^14.14.12",
    "@types/react": "^17.0.19",
    "@types/react-native": "^0.64.2",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "ava": "^3.14.0",
    "esbuild": "*",
    "eslint": "^7.32.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-react": "^7.24.0",
    "esm": "^3.2.25",
    "import-sort-style-module": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "typescript-monorepo-toolkit": "^0.2.0",
    "ultra-runner": "^3.10.5"
  },
  "optionalDependencies": {
    "prettier": "^2.3.0",
    "prettier-plugin-import-sort": "^0.0.6"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "version": "0.0.0",
  "packageManager": "yarn@3.0.2"
}
