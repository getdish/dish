{
  "name": "dish-monorepo",
  "private": true,
  "engines": {
    "node": "12.16.1"
  },
  "workspaces": {
    "packages": [
      "dish-app",
      "packages/*",
      "services/*",
      "snackui/packages/*"
    ]
  },
  "scripts": {
    "start": "echo 'run:\n yarn db\n yarn user-server\n yarn web'",
    "db": "yarn docker:start hasura",
    "db:migrate": "./dishctl.sh db_migrate_local",
    "user-server": "ultra -r --no-pretty --filter 'services/user-server' start",
    "app": "yarn workspace @dish/dish-app start",
    "web": "yarn workspace @dish/dish-app web",
    "web:prod": "yarn workspace @dish/dish-app web:prod",
    "web:prod:profile": "yarn workspace @dish/dish-app web:prod",
    "ios": "yarn workspace @dish/dish-app ios",
    "postinstall": "./bin/postinstall.sh",
    "preinstall": "./bin/preinstall.sh; ",
    "t": "eval $(./dishctl.sh yaml_to_env) echo $TF_VAR_HASURA_ENDPOINT",
    "docker:start": "eval $(./dishctl.sh yaml_to_env) docker-compose up",
    "docker:build:base": "eval $(./dishctl.sh yaml_to_env) docker-compose build base",
    "docker:build": "eval $(./dishctl.sh yaml_to_env) docker-compose build",
    "docker:start:web": "yarn stop:docker:web && yarn docker:start:web:simple & sleep 2 && yarn logs:docker:web",
    "docker:start:web:simple": "docker run --name dish-app -it -d -p 4444:4444 --rm dish/app",
    "docker:logs:web": "docker logs -f dish-app",
    "docker:stop:web": "docker rm -f dish-app || true",
    "build": "ultra -r --no-pretty --concurrency 16 build",
    "build:watch": "ultra -r --no-pretty --concurrency 500 watch",
    "build:monitor": "ultra --monitor",
    "build:watch:broken": "yarn tsc -b -w",
    "build:refs": "typescript-monorepo-toolkit inject-refs .",
    "test": "ultra -r --no-pretty --serial test",
    "graph:generate": "yarn workspace @dish/graph generate",
    "clean": "bin/clean.sh",
    "hasura:migrate": "(cd services/hasura && hasura migrate apply --endpoint http://localhost:8080 --admin-secret=password)",
    "hasura:admin": "(cd services/hasura && hasura console --endpoint http://localhost:8080 --admin-secret=password)",
    "prettier": "prettier --write \"**/*.{ts,tsx}\" || true",
    "expo:check-deps": "expo-yarn-workspaces check-workspace-dependencies",
    "snackui:pull": "git subrepo pull snackui",
    "snackui:push": "git subrepo push snackui"
  },
  "resolutions": {
    "typescript": "4.1.2",
    "snackui": "0.0.2",
    "terser": "5.3.2",
    "lodash": "4.17.20",
    "resolve": "1.17.0",
    "jsdom": "^16.4.0",
    "tslib": "1.11.2",
    "react": "0.0.0-experimental-94c0244ba",
    "react-dom": "0.0.0-experimental-94c0244ba",
    "react-test-renderer": "0.0.0-experimental-94c0244ba",
    "react-refresh": "0.0.0-experimental-94c0244ba",
    "react-native": "0.63.2",
    "react-native-web": "0.0.0-466063b7e",
    "react-native-svg": "12.1.0",
    "metro": "0.62.0",
    "metro-babel-register": "0.62.0",
    "metro-react-native-babel-preset": "0.62.0",
    "metro-config": "0.62.0",
    "metro-core": "0.62.0",
    "ts-node": "9.0.0",
    "@babel/types": "7.11.5",
    "@babel/traverse": "^7.12.1",
    "@babel/runtime": "7.11.2",
    "@babel/core": "7.11.6",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@types/react-native": "0.63.8",
    "@types/puppeteer": "3.0.1"
  },
  "dependencies": {
    "react": "0.0.0-experimental-94c0244ba",
    "react-dom": "0.0.0-experimental-94c0244ba",
    "react-native-web": "0.0.0-466063b7e",
    "tsdx": "^0.14.1"
  },
  "devDependencies": {
    "@babel/runtime": "^7.11.2",
    "@react-native-community/cli-debugger-ui": "4.7.0",
    "@types/fs-extra": "^9.0.1",
    "@types/node": "13.11.1",
    "@unimodules/core": "^5.3.0",
    "@unimodules/react-native-adapter": "^5.4.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "ava": "^3.8.2",
    "esm": "^3.2.25",
    "expo-yarn-workspaces": "*",
    "husky": "^4.3.0",
    "import-sort-style-module": "^6.0.0",
    "metro-babel-register": "0.62.0",
    "metro-config": "0.62.0",
    "metro-core": "0.62.0",
    "patch-package": "^6.2.2",
    "prettier": "^2.1.2",
    "prettier-plugin-import-sort": "^0.0.4",
    "pretty-quick": "^3.1.0",
    "react-refresh": "0.0.0-experimental-94c0244ba",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.1.2",
    "typescript-monorepo-toolkit": "^0.1.1",
    "ultra-runner": "^3.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
