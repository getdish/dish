{
  "name": "dish-monorepo",
  "private": true,
  "engines": {
    "node": ">=12 <=15"
  },
  "workspaces": {
    "packages": [
      "dish-app",
      "packages/*",
      "services/*",
      "snackui/packages/*",
      "snackui"
    ]
  },
  "scripts": {
    "env:prod": "./dishctl.sh yaml_to_env > .env",
    "trim-package": "yes | npx json-filter-cli --from=package.json --to=package.json --exclude=scripts",
    "trim-packages": "yarn trim-package && npx yarn workspaces foreach run yes | npx json-filter-cli --confirm --from=package.json --to=package.json --exclude=scripts",
    "bootstrap": "docker-compose build base",
    "start": "DOCKER_HASURA_IMAGE=fedormelexin/graphql-engine-arm64 ./dishctl.sh docker_compose_up_for_devs",
    "start:test": "rm -r ~/.dish/postgresdb/test || true; POSTGRES_DB=test docker-compose -f docker-compose-m1.yml up",
    "start:bak": "./dishctl.sh docker_compose_up_for_devs",
    "db:init": "./dishctl.sh db_migrate_local init",
    "db:migrate": "./dishctl.sh db_migrate_local",
    "db:migrate:test": "POSTGRES_DB=test ./dishctl.sh db_migrate_local",
    "app": "yarn workspace @dish/app start",
    "web": "yarn workspace @dish/app web",
    "web:prod": "yarn workspace @dish/app web:prod",
    "web:prod:optimize": "yarn workspace @dish/app web:prod:optimize",
    "web:build": "yarn workspace @dish/app web:build",
    "web:serve": "yarn workspace @dish/app web:serve",
    "etc:colorize": "yarn workspace @dish/app etc:colorize",
    "etc:colorize:staging": "yarn workspace @dish/app etc:colorize:staging",
    "ios": "yarn workspace @dish/app ios",
    "ios:install": "yarn workspace @dish/app ios:install",
    "postinstall": "yarn patch-package",
    "preinstall": "",
    "t": "eval $(./dishctl.sh yaml_to_env) && echo $HASURA_ENDPOINT",
    "deploy": "./dishctl.sh deploy remote",
    "deploy:local": "./dishctl.sh deploy local",
    "docker:build": "eval $(./dishctl.sh yaml_to_env) && docker-compose build",
    "docker:build:file": "eval $(./dishctl.sh yaml_to_env) && docker buildx build --progress=plain --platform linux/amd64",
    "docker:build:base": "yarn docker:build:file .",
    "docker:start:web": "yarn stop:docker:web && yarn docker:start:web:simple & sleep 2 && yarn logs:docker:web",
    "docker:start:web:simple": "docker run --name dish-app -it -d -p 4444:4444 --rm dish/app",
    "docker:logs:web": "docker logs -f dish-app",
    "docker:stop:web": "docker rm -f dish-app || true",
    "validate:tsc": "tsc --noEmit",
    "check:ts": "tsc -b --preserveWatchOutput tsconfig.build.json",
    "check:ts:watch": "yarn check:ts -w",
    "build:ts:watch": "ultra -r --no-pretty --parallel=100 watch",
    "build:ordered": "ultra -r --no-pretty --build",
    "build": "ultra -r --no-pretty --concurrency 30 build",
    "build:force": "ultra --silent -r --no-pretty --rebuild --concurrency 30 build",
    "build:js": "JS_ONLY=1 yarn workspaces foreach -piR run build",
    "build:docker": "./dishctl.sh buildkit_build_local",
    "watch": "npm-run-all --parallel check:ts:watch build:ts:watch build:extras:watch",
    "build:refs": "typescript-monorepo-toolkit inject-refs .",
    "build:extras:watch": "ultra -r --no-pretty --filter @snackui/node watch",
    "test": "ultra -r --no-pretty --serial test",
    "crawl:refresh-google": "eval $(./dishctl.sh yaml_to_env) && GOOGLE_AWS_PROXY=$GOOGLE_AWS_PROXY LUMINATI_PROXY_HOST=$LUMINATI_PROXY_HOST LUMINATI_PROXY_PORT=$LUMINATI_PROXY_PORT LUMINATI_PROXY_DATACENTRE_USER=$LUMINATI_PROXY_DATACENTRE_USER LUMINATI_PROXY_DATACENTRE_PASSWORD=$LUMINATI_PROXY_DATACENTRE_PASSWORD LUMINATI_PROXY_RESIDENTIAL_USER=$LUMINATI_PROXY_RESIDENTIAL_USER LUMINATI_PROXY_RESIDENTIAL_PASSWORD=$LUMINATI_PROXY_RESIDENTIAL_PASSWORD HASURA_SECRET=$HASURA_GRAPHQL_ADMIN_SECRET node services/crawlers/_/google/update_search_endpoint.js",
    "crawl:one": "eval $(./dishctl.sh yaml_to_env) && HASURA_SECRET=$HASURA_GRAPHQL_ADMIN_SECRET RUN_WITHOUT_WORKER=true DISH_ENV=development DISABLE_LUMINATI=1 node services/crawlers/_/yelp/sandbox.js",
    "crawl:internal": "eval $(./dishctl.sh yaml_to_env) && HASURA_SECRET=$HASURA_GRAPHQL_ADMIN_SECRET  RUN_WITHOUT_WORKER=true DISH_ENV=development QUERY=\"SELECT id from restaurant $QUERY\" node ./services/crawlers/_/self/sandbox.js",
    "crawl:internal:slug": "QUERY=\"WHERE slug = '$SLUG'\" yarn crawl:internal",
    "graph:generate": "yarn workspace @dish/graph generate",
    "clean": "./dishctl.sh clean",
    "clean:build": "./dishctl.sh clean_build",
    "hasura:migrate": "./dishctl.sh db_migrate_local init",
    "hasura:admin": "(cd services/hasura && hasura console --endpoint http://localhost:8080 --admin-secret=${HASURA_GRAPHQL_ADMIN_SECRET:-password})",
    "prettier": "prettier --write \"**/*.{ts,tsx}\" || true",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "expo:check-deps": "expo-yarn-workspaces check-workspace-dependencies",
    "snackui:pull": "git subrepo pull snackui",
    "snackui:push": "git subrepo clean snackui && git subrepo push snackui",
    "react-native-if-you-want-it": "0.63.2"
  },
  "resolutions": {
    "react-native": "0.0.0",
    "@unimodules/core": "7.0.0",
    "aws-sdk": "2.762.0",
    "typescript": "4.2.3",
    "node-fetch": "2.6.1",
    "webpack": "5.24.2",
    "terser": "5.5.1",
    "lodash": "4.17.20",
    "resolve": "1.17.0",
    "jsdom": "^16.4.0",
    "tslib": "2.0.3",
    "react": "17.0.1",
    "expo-yarn-workspaces": "1.3.1",
    "react-dom": "17.0.1",
    "react-test-renderer": "17.0.1",
    "react-refresh": "0.9.0",
    "react-native-web": "0.0.0-466063b7e",
    "react-native-svg": "",
    "metro": "0.62.0",
    "metro-babel-register": "0.62.0",
    "metro-react-native-babel-preset": "0.62.0",
    "metro-react-native-babel-transformer": "0.62.0",
    "metro-cache": "0.62.0",
    "metro-config": "0.62.0",
    "metro-core": "0.62.0",
    "@babel/core": "7.12.9",
    "@babel/generator": "7.12.5",
    "@babel/parser": "7.12.7",
    "@babel/traverse": "7.12.9",
    "@babel/types": "7.12.7",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@types/react-native": "0.63.45",
    "@types/puppeteer": "3.0.1",
    "esbuild": "0.8.54"
  },
  "dependencies": {
    "@dish/app": "*",
    "@yarnpkg/plugin-patch": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-native-web": "0.0.0-466063b7e",
    "react-refresh": "*",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.2.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.12.5",
    "@types/fs-extra": "^9.0.8",
    "@types/node": "^14.14.12",
    "@types/react-native": "^0.63.45",
    "ava": "^3.14.0",
    "esbuild": "^0.8.56",
    "esm": "^3.2.25",
    "import-sort-style-module": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.6",
    "react-native": "^0.62.2",
    "typescript-monorepo-toolkit": "^0.2.0",
    "ultra-runner": "^3.10.5"
  },
  "optionalDependencies": {
    "prettier": "^2.2.1",
    "prettier-plugin-import-sort": "^0.0.6"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "version": "0.0.0"
}
