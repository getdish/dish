{
  "name": "@dish/ui",
  "version": "0.0.1",
  "main": "_/index.js",
  "tsmain": "src/index",
  "sideEffects": false,
  "private": true,
  "workspaces": {
    "nohoist": [
      "**"
    ]
  },
  "scripts": {
    "build": "npm-run-all --parallel build:main build:web",
    "build:main": "tsc -b || true",
    "build:web": "ttsc -p tsconfig.node.json || true",
    "watch": "npm-run-all --parallel watch:main watch:web",
    "watch:main": "tsc -b --watch --preserveWatchOutput",
    "watch:web": "ttsc -p tsconfig.node.json --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@dish/async": "*",
    "@o/fast-compare": "^2.7.6",
    "lodash": "^4.17.15",
    "react-laag": "^1.7.3",
    "react-storage-hooks": "^4.0.0",
    "react-feather": "2.0.8",
    "react-native-web": "^0.12.3"
  },
  "peerDependencies": {
    "react-native": "*",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "ttypescript": "1.5.10",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "npm-run-all": "^4.1.5"
  }
}
