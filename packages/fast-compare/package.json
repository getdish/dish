{
  "name": "@dish/fast-compare",
  "version": "2.7.6",
  "description": "none",
  "main": "_/fastCompare.js",
  "module": "_/esm/fastCompare.js",
  "ts:main": "src/fastCompare.ts",
  "scripts": {
    "build": "npm-run-all --parallel build:main build:module",
    "build:main": "tsc -b",
    "build:module": "yarn tsc -b tsconfig.esm.json",
    "watch": "npm-run-all --parallel watch:main watch:module",
    "watch:main": "tsc -b --watch --preserveWatchOutput",
    "watch:module": "yarn tsc -b tsconfig.esm.json --watch --preserveWatchOutput"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "sideEffects": false
}
