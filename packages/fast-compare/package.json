{
  "name": "@dish/fast-compare",
  "version": "2.7.6",
  "description": "none",
  "main": "_/fastCompare.js",
  "module": "_/esm/fastCompare.js",
  "ts:main": "src/fastCompare.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rm -f tsconfig.tsbuildinfo",
    "build": "npm-run-all --parallel build:main build:module",
    "build:main": "tsc",
    "build:module": "yarn tsc -p tsconfig.esm.json",
    "watch": "npm-run-all --parallel watch:main watch:module",
    "watch:main": "tsc --watch --preserveWatchOutput",
    "watch:module": "yarn tsc -p tsconfig.esm.json --watch --preserveWatchOutput"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  },
  "sideEffects": false
}
