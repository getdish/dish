{
  "name": "@dish/server",
  "version": "0.0.1",
  "main": "_",
  "tsmain": "src",
  "sideEffects": false,
  "private": true,
  "bin": {
    "dish": "./bin/run"
  },
  "files": [
    "/bin",
    "/_"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.json",
    "watch": "tsc -b tsconfig.json --watch --preserveWatchOutput"
  },
  "oclif": {
    "commands": "./_/commands",
    "bin": "dish",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete"
    ]
  },
  "dependencies": {
    "@dish/graph": "*",
    "ts-node": "^9.1.1",
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/plugin-autocomplete": "0.3.0",
    "@oclif/plugin-help": "3.2.1",
    "@oclif/plugin-not-found": "1.2.4",
    "compression": "^1.7.4",
    "enquirer": "^2.3.6",
    "express": "^4.17.1",
    "devcert": "^1.1.3",
    "react-ssr-prepass": "^1.2.1",
    "@loadable/server": "^5.14.0",
    "@loadable/component": "^5.14.1",
    "body-parser": "^1.19.0",
    "browserslist-useragent": "^3.0.3",
    "isomorphic-unfetch": "^3.1.0",
    "jsdom": "^16.4.0",
    "fs-extra": "^9.0.1",
    "react-helmet": "^6.1.0",
    "@dish/helpers": "*",
    "express-remove-route": "^1.0.0",
    "chokidar": "^3.5.0",
    "lodash": "*",
    "webpack-pwa-manifest": "^4.3.0",
    "webpack": "^5.14.0",
    "express-winston": "^4.0.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "webpack-dev-middleware": "^4.0.3",
    "connect-history-api-fallback": "^1.6.0",
    "webpack-hot-middleware": "^2.25.0",
    "@types/express": "^4.17.10",
    "@types/jsdom": "^16.2.5"
  }
}
