{
  "name": "@dish/graph",
  "version": "0.0.1",
  "sideEffects": false,
  "private": true,
  "types": "types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "files": [
    "types",
    "src",
    "dist"
  ],
  "testConfig": {
    "isolate": true
  },
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "generate": "DISH_ENV=development ../../dsh run 'node ./dist/cjs/generate.js'",
    "test": "NODE_ENV=test TARGET=node ava test/* --verbose --serial",
    "test:only": "NODE_ENV=test TARGET=node ava test/* --verbose --serial",
    "test:debug": "NODE_ENV=test TARGET=node DEBUG=1 ava test/* --verbose --serial",
    "test:watch": "NODE_ENV=test TARGET=node ava test/* --watch --verbose --serial",
    "test-new": "NODE_ENV=test TARGET=node ava test/tag.ts --verbose"
  },
  "dependencies": {
    "@dish/helpers": "*",
    "@gqty/logger": "^2.0.2",
    "@gqty/react": "^2.1.0",
    "@tamagui/use-store": "^1.26.0",
    "cross-local-storage": "^0.0.5",
    "esbuild-register": "^3.3.3",
    "gqty": "^2.4.0-alpha-c5b89dbd.0",
    "graphql": "^15.5.0",
    "lodash": "*",
    "moment": "^2.29.1",
    "react": "*"
  },
  "devDependencies": {
    "@dish/helpers-node": "*",
    "@dish/react-test-env": "*",
    "@gqty/cli": "^3.1.0",
    "@tamagui/build": "^1.26.0",
    "@types/node": "^16.11.10",
    "ava": "^3.15.0",
    "localstorage-polyfill": "^1.0.1"
  }
}
