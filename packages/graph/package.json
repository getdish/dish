{
  "name": "@dish/graph",
  "version": "0.0.1",
  "main": "_/index.js",
  "tsmain": "src/index",
  "sideEffects": false,
  "private": true,
  "workspaces": {
    "nohoist": [
      "**"
    ]
  },
  "scripts": {
    "prebuild": "yarn generate",
    "generate": "(cd ../common-web && yarn build) && ./generate.sh",
    "build": "tsc -b || true",
    "watch": "tsc -b --watch --preserveWatchOutput",
    "test": "true",
    "postinstall": "ls -la node_modules; la -la node_modules/gqless; patch-package"
  },
  "dependencies": {
    "patch-package": "^6.2.2",
    "@dish/auth": "*",
    "@dish/common-web": "*",
    "isomorphic-unfetch": "^3.0.0",
    "@gqless/logger": "0.0.1-alpha.27",
    "gqless-hooks": "^1.1.1",
    "gqless": "^0.0.1-alpha.29",
    "@gqless/schema": "^0.0.1-alpha.31",
    "react": "0.0.0-experimental-aae83a4b9"
  },
  "devDependencies": {
    "@types/node": "*",
    "@types/react": "*"
  }
}
