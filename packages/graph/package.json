{
  "name": "@dish/graph",
  "version": "0.0.1",
  "main": "_/index.js",
  "tsmain": "src/index",
  "sideEffects": false,
  "private": true,
  "workspaces": {
    "nohoist": [
      "**"
    ]
  },
  "scripts": {
    "prebuild": "yarn generate",
    "generate": "(cd ../common-web && yarn build) && ls || true && ./generate.sh",
