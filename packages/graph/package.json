{
  "name": "@dish/graph",
  "version": "0.0.1",
  "sideEffects": false,
  "private": true,
  "types": "types",
  "main": "dist",
  "module": "_",
  "scripts": {
    "build": "esdx legacy",
    "watch": "esdx-watch legacy",
    "generate": "DISH_ENV=development ../../dishctl.sh run 'node ./dist/generate.js'",
    "test": "NODE_ENV=test TARGET=node ava test/* --verbose --serial",
    "test:only": "NODE_ENV=test TARGET=node ava test/* --verbose --serial",
    "test:debug": "NODE_ENV=test TARGET=node DEBUG=1 ava test/* --verbose --serial",
    "test:watch": "NODE_ENV=test TARGET=node ava test/* --watch --verbose --serial",
    "test-new": "NODE_ENV=test TARGET=node ava test/tag.ts --verbose"
  },
  "dependencies": {
    "@dish/helpers": "*",
    "@dish/use-store": "*",
    "@gqless/logger": "^2.0.11",
    "@gqless/react": "^2.0.12",
    "esbuild-register": "*",
    "gqless": "^2.0.13",
    "graphql": "^15.5.0",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "*",
    "moment": "^2.29.1",
    "react": "*"
  },
  "devDependencies": {
    "@dish/esdx": "*",
    "@dish/helpers-node": "*",
    "@dish/react-test-env": "*",
    "@gqless/cli": "^2.0.15",
    "ava": "^3.14.0"
  }
