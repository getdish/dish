{
  "name": "@dish/graph",
  "version": "0.0.1",
  "main": "_",
  "tsmain": "src",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "prebuild": "rm -f tsconfig.tsbuildinfo",
    "generate": "HASURA_SECRET=password ts-node --transpile-only ./generate.ts",
    "build": "tsc -b",
    "watch": "tsc -b --watch --preserveWatchOutput",
    "test": "TARGET=node DISH_ENV=test ava test/* --verbose --serial",
    "test:debug": "TARGET=node DEBUG=1 DISH_ENV=test ava test/* --verbose --serial",
    "test:watch": "TARGET=node DISH_ENV=test ava test/* --watch --verbose --serial",
    "test-new": "TARGET=node DISH_ENV=test ava test/tag.ts --verbose"
  },
  "dependencies": {
    "@dish/helpers": "*",
    "patch-package": "^6.2.2",
    "isomorphic-unfetch": "3.1.0",
    "ava": "^3.14.0",
    "lodash": "*",
    "@dish/gqless": "3.0.4",
    "@dish/gqless-react": "2.0.5",
    "@dish/gqless-logger": "0.1.3",
    "@dish/use-store": "*"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@dish/react-test-env": "*",
    "@types/node": "*",
    "ts-node": "*",
    "@dish/gqless-cli": "4.0.0"
  }
}
