{
  "name": "@dish/graph",
  "version": "0.0.1",
  "sideEffects": false,
  "private": true,
  "types": "types",
  "main": "dist",
  "testConfig": {
    "isolate": true
  },
  "module": "_",
  "scripts": {
    "build": "esdx legacy legacy",
    "watch": "esdx-watch legacy",
    "generate": "DISH_ENV=development ../../dsh run 'node ./dist/generate.js'",
