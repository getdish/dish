{
  "name": "@dish/graph",
  "version": "0.0.1",
  "sideEffects": false,
  "private": true,
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typings": "types",
  "testConfig": {
    "isolate": true
  },
  "scripts": {
    "build": "dish-build legacy",
    "watch": "dish-build --watch legacy",
    "generate": "DISH_ENV=development ../../dsh run 'node ./dist/generate.js'",
    "test": "NODE_ENV=test TARGET=node ava test/* --verbose --serial",
    "test:only": "NODE_ENV=test TARGET=node ava test/* --verbose --serial",
    "test:debug": "NODE_ENV=test TARGET=node DEBUG=1 ava test/* --verbose --serial",
    "test:watch": "NODE_ENV=test TARGET=node ava test/* --watch --verbose --serial",
    "test-new": "NODE_ENV=test TARGET=node ava test/tag.ts --verbose"
  },
  "dependencies": {
    "@dish/helpers": "*",
    "@dish/use-store": "*",
    "@gqty/logger": "^2.0.0",
    "@gqty/react": "^2.0.0",
    "esbuild-register": "*",
    "gqty": "2.0.2",
    "graphql": "^15.5.0",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "*",
    "moment": "^2.29.1",
    "react": "*"
  },
  "devDependencies": {
    "@dish/build": "^1.0.0-alpha.2",
    "@dish/helpers-node": "*",
    "@dish/react-test-env": "*",
    "@gqty/cli": "^2.0.1",
    "@types/node": "^16.11.8",
    "ava": "^3.14.0"
  }
}
