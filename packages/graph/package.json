{
  "name": "@dish/graph",
  "version": "0.0.1",
  "main": "_/index.js",
  "tsmain": "src/index",
  "sideEffects": false,
  "private": true,
  "workspaces": {
    "nohoist": [
      "gqless"
    ]
  },
  "scripts": {
    "prebuild": "yarn generate",
    "generate": "(cd ../common-web && yarn build) && ./generate.sh",
    "build": "tsc -b || true",
    "watch": "tsc -b --watch --preserveWatchOutput",
    "test": "true",
    "postinstall": "patch-package",
    "postinstall-verbose": "patch --verbose -t -p1 -i patches/gqless+0.0.1-alpha.29.patch"
  },
  "dependencies": {
    "@gqless/react": "0.0.1-alpha.28",
    "patch-package": "^6.2.2",
    "@dish/auth": "*",
    "@dish/common-web": "*",
    "isomorphic-unfetch": "^3.0.0",
    "@gqless/logger": "0.0.1-alpha.27",
    "gqless-hooks": "^1.1.1",
    "gqless": "^0.0.1-alpha.29",
    "@gqless/schema": "^0.0.1-alpha.31"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@types/node": "*"
  }
}
