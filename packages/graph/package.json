    "graphql": "^15.5.0",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "*",
    "moment": "^2.29.1",
    "react": "*"
  },
  "devDependencies": {
    "@dish/esdx": "*",
    "@dish/helpers-node": "*",
    "@dish/react-test-env": "*",
    "@gqless/cli": "^2.0.6",
    "ava": "^3.14.0"
