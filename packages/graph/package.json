{
  "name": "@dish/graph",
  "version": "0.0.1",
  "main": "_",
  "tsmain": "src",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "generate": "ts-node ./generate.ts",
    "build": "tsc -b || true",
    "watch": "tsc -b --watch --preserveWatchOutput",
    "test": "TARGET=node DISH_ENV=test ava test/* --verbose --serial",
    "test:debug": "TARGET=node DEBUG=1 DISH_ENV=test ava test/* --verbose --serial",
    "test:watch": "TARGET=node DISH_ENV=test ava test/* --watch --verbose --serial"
  },
  "dependencies": {
    "@gqless/react": "0.0.1-alpha.28",
    "patch-package": "^6.2.2",
    "isomorphic-unfetch": "^3.0.0",
    "@gqless/logger": "0.0.1-alpha.27",
    "ava": "^3.8.2",
    "gqless": "^0.0.1-alpha.29",
    "@gqless/schema": "0.0.1-alpha.31",
    "lodash": "*"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@dish/react-test-env": "*",
    "@types/node": "*",
    "ts-node": "*"
  }
}
