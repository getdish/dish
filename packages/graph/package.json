{
  "name": "@dish/graph",
  "version": "0.0.1",
  "main": "_/src/index.js",
  "tsmain": "src/index",
  "sideEffects": false,
  "private": true,
  "workspaces": {
    "nohoist": [
      "gqless"
    ]
  },
  "scripts": {
    "generate": "./generate.sh",
    "build": "tsc -b || true",
    "watch": "tsc -b --watch --preserveWatchOutput",
    "test": "DISH_ENV=test ava test/* --verbose --serial",
    "test:watch": "DISH_ENV=test ava test/* --watch --verbose --serial",
    "postinstall": "patch-package || yarn postinstall-verbose",
    "postinstall-verbose": "patch --verbose -t -p1 -i patches/gqless+0.0.1-alpha.29.patch"
  },
  "dependencies": {
    "@gqless/react": "0.0.1-alpha.28",
    "patch-package": "^6.2.2",
    "isomorphic-unfetch": "^3.0.0",
    "@gqless/logger": "0.0.1-alpha.27",
    "gqless-hooks": "^1.1.1",
    "gqless-hook": "^0.0.4",
    "ava": "^3.8.2",
    "gqless": "^0.0.1-alpha.29",
    "@o/gqless-schema": "0.0.1-alpha.32"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@types/node": "*",
    "ts-node": "^8.10.1"
  }
}
