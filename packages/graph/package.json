{
  "name": "@dish/graph",
  "version": "0.0.1",
  "main": "_",
  "tsmain": "src",
  "sideEffects": false,
  "private": true,
  "workspaces": {
    "nohoist": [
      "gqless",
      "@gqless/logger",
      "@o/gqless-schema",
      "@gqless/schema"
    ]
  },
  "scripts": {
    "generate": "ts-node ./generate.ts",
    "build": "tsc -b || true",
    "watch": "tsc -b --watch --preserveWatchOutput",
    "test": "DISH_ENV=test ava test/* --verbose --serial",
    "test:debug": "DEBUG=1 DISH_ENV=test ava test/* --verbose --serial",
    "test:watch": "DISH_ENV=test ava test/* --watch --verbose --serial",
    "postinstall": "patch-package || yarn postinstall-verbose",
    "postinstall-verbose": "patch --verbose -t -p1 -i patches/gqless+0.0.1-alpha.29.patch"
  },
  "dependencies": {
    "node-localstorage": "^2.1.6",
    "@gqless/react": "0.0.1-alpha.28",
    "patch-package": "^6.2.2",
    "isomorphic-unfetch": "^3.0.0",
    "@gqless/logger": "0.0.1-alpha.27",
    "ava": "^3.8.2",
    "gqless": "^0.0.1-alpha.29",
    "@o/gqless-schema": "0.0.1-alpha.34",
    "@gqless/schema": "0.0.1-alpha.31",
    "lodash": "*"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@dish/react-test-env": "*",
    "@types/node": "*",
    "ts-node": "^8.10.1"
  }
}
