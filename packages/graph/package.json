{
  "name": "@dish/graph",
  "version": "0.0.1",
  "main": "_",
  "tsmain": "src",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "generate": "ts-node --transpile-only ./generate-new.ts",
    "build": "tsc -b || true",
    "watch": "tsc -b --watch --preserveWatchOutput",
    "test": "TARGET=node DISH_ENV=test ava test/* --verbose --serial",
    "test:debug": "TARGET=node DEBUG=1 DISH_ENV=test ava test/* --verbose --serial",
    "test:watch": "TARGET=node DISH_ENV=test ava test/* --watch --verbose --serial",
    "test-new": "TARGET=node DISH_ENV=test ava test/tag.ts --verbose"
  },
  "dependencies": {
    "@dish/helpers": "*",
    "@o/gqless": "*",
    "@o/gqless-react": "*",
    "@o/gqless-schema": "*",
    "@o/gqless-logger": "*",
    "patch-package": "^6.2.2",
    "isomorphic-unfetch": "3.1.0",
    "ava": "^3.8.2",
    "lodash": "*",
    "@dish/gqless": "^1.0.19",
    "@dish/gqless-react": "^0.1.4"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@o/react-test-env": "*",
    "@types/node": "*",
    "ts-node": "*",
    "@dish/gqless-cli": "^0.1.22"
  }
}
