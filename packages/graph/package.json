    "esbuild-register": "*",
    "gqty": "1.0.3",
    "graphql": "^15.5.0",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "*",
    "moment": "^2.29.1",
    "react": "*"
  },
  "devDependencies": {
    "@dish/esdx": "*",
    "@dish/helpers-node": "*",
    "@dish/react-test-env": "*",
    "@gqty/cli": "1.0.4",
    "@types/node": "*",
    "ava": "^3.14.0"
  }
