{
  "compilerOptions": {
    "importHelpers": true,
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "preserveSymlinks": true,
    "incremental": true,
    "jsx": "react",
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmitOnError": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveConstEnums": true,
    "removeComments": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "strictNullChecks": true,
    "target": "es2020",
    "types": ["node", "jest"],
    "lib": ["es2020", "dom", "esnext"]
  },
  "exclude": ["_", "test/**/*"],
  "typeAcquisition": {
    "enable": true
  }
}
